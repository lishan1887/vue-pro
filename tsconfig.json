{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "strict": true,
      "jsx": "preserve",
      "importHelpers": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": false,
      "baseUrl": ".",
      "types": [
        "webpack-env",
        "jest"
      ],
      "paths": {
        "@/*": [
          "src/*"
        ],
        "vant/lib/count-down": ["src/components/Vant/count-down"],
        "vant/lib/tabs": ["src/components/Vant/tabs"],
        "vant/lib/noticeBar": ["src/components/Vant/noticeBar"],
        "vant/lib/tab ": ["src/components/Vant/tab "],
        "vant/lib/Slider ": ["src/components/Vant/Slider"],
        "vant/lib/tabbar": ["src/components/Vant/tabbar"],
        "vant/lib/tabbar-item": ["src/components/Vant/tab"],
        "vant/lib/sidebar": ["src/components/Vant/sidebar"],
        "vant/lib/sidebar-item": ["src/components/Vant/sidebar"]
      },
      "lib": [
        "esnext",
        "dom",
        "dom.iterable",
        "scripthost"
      ]
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.vue",
      "tests/**/*.ts",
      "tests/**/*.tsx"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  